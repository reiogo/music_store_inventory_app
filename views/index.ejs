<html>
  <head>
    <link rel="stylesheet" href="/styles.css">
    <title><%= title %></title>
  </head>
  <body>
    <%- include('navbar', {links:links}) %>
    <h1> <%= title %></h1>
    <% if (locals.instruments) { %>
        <% instruments.forEach((instrument)=>{ %>
        <div class="instrument-div">
            <p><%= instrument.name %></p>
            <p><%= instrument.maker %></p>
            <p><%= instrument.price %></p>
        </div>
        <% }); %>
        <% } %>
  </body>
</html>
